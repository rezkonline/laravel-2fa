{
    "name": "rezkonline/laravel-2fa",
    "description": "A simple 2FA authentication for laravel applications.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mateus Rezkonline",
            "email": "mateus@Rezkonline.dev"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/auth": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Rezkonline\\TwoFactorAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rezkonline\\TwoFactorAuth\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}
